<h2>Edit <%= resource_name.to_s.humanize %></h2>
<%= link_to "Back", owners_authenticated_root_path %>
<br>
<br>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
  <%= render "owners/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %>: <%= resource.email %><br />
  </div>


  <div class="field">
    <%= f.label :club_name %><br />
    <%= f.text_field :club_name %>
  </div>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>